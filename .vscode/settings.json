{
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "C_Cpp.autocompleteAddParentheses": true,
    "editor.tabSize": 2,
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}"
    ],
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "./**/*{test,Test,TEST}*",
            "cwd": "${workspaceFolder}",
            "env": {}
        }
    ],
    "files.associations": {
        "array": "cpp",
        "string_view": "cpp",
        "regex": "cpp",
        "iostream": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "sstream": "cpp",
        "list": "cpp",
        "queue": "cpp"
    }
}